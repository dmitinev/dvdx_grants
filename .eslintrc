{
  "rules": {
    "no-unused-vars": "warn",
    "jsx-quotes": [
      1,
      "prefer-double"
    ],
    "no-console": "warn",
    "no-undef": "off"
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false,
    "babelOptions": {
      "babelrc": false,
      "configFile": false,
      "presets": ["@babel/preset-env"]
    }
  },
  "extends": ["eslint:recommended"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  }
}